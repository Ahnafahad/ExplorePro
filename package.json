{
  "name": "explorepro",
  "version": "1.0.0",
  "type": "module",
  "description": "ExplorePro - Uber for Tour Guides",
  "private": true,
  "scripts": {
    "install:all": "npm install --prefix frontend && npm install --prefix backend",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:backend": "npm run dev --prefix backend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "npm run build --prefix frontend",
    "build:backend": "npm run build --prefix backend",
    "build": "npm run build:backend && npm run build:frontend",
    "prisma:generate": "npm run prisma:generate --prefix backend",
    "prisma:migrate": "npm run prisma:migrate --prefix backend",
    "prisma:studio": "npm run prisma:studio --prefix backend"
  },
  "keywords": [
    "tour-guides",
    "marketplace",
    "booking",
    "tourism"
  ],
  "author": "Ahnaf Ahad",
  "license": "UNLICENSED",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
